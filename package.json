    "axios": "^1.7.9",
    "co-body": "^6.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "ulidx": "^2.4.1",
    "uuid": "^11.0.3",
    "validator": "^13.12.0",
