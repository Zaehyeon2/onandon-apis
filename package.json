{
  "name": "onandon-apis",
  "version": "0.0.1",
  "description": "lambda apis for onandon service",
  "author": "insam2802@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\" \"bin/**/*.ts\" \"stacks/**/*.ts\"",
    "lint": "eslint \"{apps,libs,src,test,stacks}/**/*.ts\" --fix",
    "all:clean": "yarn app:clean && yarn cdk:clean",
    "all:build": "nest build api && nest build log-processor",
    "cdk:clean": "rimraf cdk.out cdk.context.json .aws-sam",
    "cdk:local": "cdk synth onandon-apis-local --no-staging",
    "cdk:local:macos": "cross-env ARCH=arm cdk synth onandon-apis-local --no-staging",
    "sam:local": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 sam local start-api --warm-containers lazy -t ./cdk.out/onandon-apis-local.template.json",
    "app:clean": "rimraf dist",
    "api:build": "nest build api --watch",
    "api:local": "cross-env RUN_MODE=standalone NODE_TLS_REJECT_UNAUTHORIZED=0 nest start --watch api",
    "api:local:js": "cross-env RUN_MODE=standalone node dist/apps/api/main",
    "api:test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "^2.0.9",
    "@anatine/zod-openapi": "^2.2.6",
    "@aws-sdk/client-dynamodb": "^3.714.0",
    "@aws-sdk/client-lambda": "^3.714.0",
    "@aws-sdk/client-s3": "^3.715.0",
    "@aws-sdk/client-sfn": "^3.714.0",
    "@aws-sdk/client-sqs": "^3.714.0",
    "@aws-sdk/lib-dynamodb": "^3.714.0",
    "@codegenie/serverless-express": "^4.16.0",
    "@elastic/ecs-winston-format": "^1.5.3",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/testing": "^10.4.15",
    "@opensearch-project/opensearch": "^2.13.0",
